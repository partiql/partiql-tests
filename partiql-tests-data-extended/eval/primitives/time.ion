// The behavior for precision & truncating is implementation-defined
// Tests below following PLK implementations
time::[
  {
    name: "explicit time truncate",
    statement: "TIME WITHOUT TIME ZONE '23:59:59.999999999999'",
    assert: {
      evalMode: [EvalModeCoerce, EvalModeError],
      result: EvaluationSuccess,
      output: $time::{ hour: 23, minute: 59, second: 59.999999999, offset: null }
    }
  },
]

timez::[
  {
    name: "timez truncate",
    statement: "TIME WITH TIME ZONE '23:59:59.999999999999+00:00'",
    assert: {
      evalMode: [EvalModeCoerce, EvalModeError],
      result: EvaluationSuccess,
      output: $time::{ hour: 23, minute: 59, second: 59.999999999, offset: 0 }
    }
  },
]