    assert:{
      result:EvaluationSuccess,
      evalMode:[
        EvalModeCoerce,
        EvalModeError
      ],
      output:$missing::null
    }
  },
  {
    name:"OCTET_LENGTH MISSING",
    statement:"OCTET_LENGTH(MISSING)",
    assert:{
      result:EvaluationSuccess,
