parse::{
    name: "CAST as VARCHAR",
    statement: "CAST(5 AS VARCHAR)",
    assert: [
        {
            result: ParseOk
        },
    ]
}

parse::{
    name: "CAST as VARCHAR with length",
    statement: "CAST(5 AS VARCHAR(5))",
    assert: [
        {
            result: ParseOk
        },
    ]
}

