parse::{
    name: "PIVOT with only FROM",
    statement: "PIVOT v AT n FROM data",
    assert: {
        result: ParseOk
    }
}

parse::{
    name: "PIVOT HAVING with WHERE and GROUP BY",
    statement: "PIVOT g AT ('prefix:' || c) FROM data WHERE a = b GROUP BY c, d GROUP AS g HAVING d > 6",
    assert: {
        result: ParseOk
    }
}
