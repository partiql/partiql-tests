parse::{
    name: "at operator without identifier",
    statement: "@",
    assert: {
        result: ParseError
    },
}

parse::{
    name: "at operator on non identifier",
    statement: "@(a)",
    assert: {
        result: ParseError
    },
}

parse::{
    name: "double at operator on identifier",
    statement: "@ @a",
    assert: {
        result: ParseError
    },
}
